<h1> Edit Job </h1>

  <%= simple_form_for @job do |f| %>

  <%= f.input :title %>
  <%= f.input :description %>
  <%= f.input :wage_lower_bound, :label => "Wage Lower Bound" %>
  <%= f.input :wage_upper_bound, :label => "Wage Upper Bound" %>
  <%= f.input :contact_email %>

  <%= f.submit "Submit", :class => "btn btn-primary" %>
  <% end %>
